		<!-- Un élément HTML pour recueillir la carte -->
		<div id="map" style="box-shadow:2px 2px 2px 2px gray;border-radius:10px 10px 10px 10px;width:470px;height:480px"></div>

		<!-- Chargement de l'API Google maps -->
		<script src="http://maps.google.com/maps/api/js?sensor=false"></script>
<br/>
		<center><form>
			<select id='MonSelect' onchange="position()" style="box-shadow:2px 2px 2px 2px gray;border-radius:10px 10px 10px 10px;">
				<option selected="selected"> Choisissez l'un des clubs</option>
				<option> Club Jeune science central</option>
				<option> Club Jeune science de Tunis</option>
				<option> Club Jeune science El Kef</option>
				<option> Club Jeune science La Goulette</option>
				<option> Club Jeune science Medjez El Beb</option>
				<option> Club Jeune science Hamem Sousse</option>
				<option> Club Jeune science El Alia</option>
			</select>
		</form></center><br/>
		<script>
			var centerpos = new google.maps.LatLng(36.835668,10.162354);

			// Ansi que des options pour la carte, centrée sur latlng
			var optionsGmaps = {
				center : centerpos,
				navigationControlOptions : {
					style : google.maps.NavigationControlStyle.SMALL
				},
				mapTypeId : google.maps.MapTypeId.ROADMAP,
				zoom : 16
			};

			// Initialisation de la carte avec les options
			var map = new google.maps.Map(document.getElementById("map"), optionsGmaps);

			// Marqueur
			var marker;

			// Coordonnées
			var latlng;

			// Fonction de callback en cas de succès
			function surveillePosition(position) {
				// On instancie un nouvel objet LatLng pour Google Maps
				latlng = new google.maps.LatLng(position.coords.latitude, position.coords.longitude);

				// Ajout d'un marqueur à la position trouvée
				if ( typeof marker == 'undefined') {
					marker = new google.maps.Marker({
						position : latlng,
						map : map,
						title : "Vous êtes ici"
					});
				} else {
					marker.setPoint(latlng);
				}

				map.panTo(latlng);

			}

			// Fonction de callback en cas d’erreur
			var survId;

			survId = navigator.geolocation.watchPosition(surveillePosition);

			function showPosition(lngt, lat) {
				var myLatlng = new google.maps.LatLng(lngt, lat);
				var mapOptions = {
					zoom : 16,
					center : myLatlng,
					mapTypeId : google.maps.MapTypeId.ROADMAP,
				}
				var map = new google.maps.Map(document.getElementById("map"), mapOptions);

				var marker = new google.maps.Marker({
					position : myLatlng,
					title : document.getElementById('MonSelect').options[document.getElementById('MonSelect').selectedIndex].value
				});

				// To add the marker to the map, call setMap();
				marker.setMap(map);
			}

			function position() {
				if (navigator.geolocation) {
					var selectValue = document.getElementById('MonSelect').options[document.getElementById('MonSelect').selectedIndex].value;
					if (selectValue == 'Club Jeune science central') {
						showPosition(36.831014, 10.18836);
					} else if (selectValue == 'Club Jeune science de Tunis') {
						showPosition(36.799947, 10.187346);
					} else if (selectValue == 'Club Jeune science El Kef') {
						showPosition(36.181346, 8.707338);
					} else if (selectValue == 'Club Jeune science La Goulette')
					{
						showPosition(36.828474,10.312005);
					} else if (selectValue == 'Club Jeune science Medjez El Beb') {
						showPosition(36.649663, 9.610303);
					} else if (selectValue == 'Club Jeune science Hamem Sousse')
					{
						showPosition(35.86185,10.593057);
					} else if (selectValue == 'Club Jeune science El Alia') {
						showPosition(37.16955, 10.033956);
					} else {
						alert("choisissez l'un des clubs!!");
					}
				}
			}
		</script>

